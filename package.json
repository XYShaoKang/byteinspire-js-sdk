{
  "name": "@byteinspire/js-sdk",
  "version": "0.2.2",
  "description": "inspirecloud javascript sdk",
  "main": "./dist/node/src/index.js",
  "types": "./dist/node/src/index.d.ts",
  "keywords": [
    "inspirecloud",
    "InspireCloud",
    "byteinspire",
    "sdk",
    "serverless",
    "faas",
    "qingfuwu",
    "QINGFUWU",
    "functions",
    "database",
    "storage"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "eslint src/**/*.ts test/**/*.ts --quiet --fix",
    "test:node": "jest -c jest/node.js --env=./jest/jest-environment-custom.js",
    "test:browser": "jest -c jest/browser.js",
    "test": "npm run test:mp && npm run test:browser && npm run test:node",
    "test:mp": "jest -c jest/mp.js --env=./jest/jest-enviroment-mp.js",
    "build:node": "tsc -p tsconfig.json -d",
    "build": "npm run clean && npm run build:node && npm run build:client",
    "build:client": "webpack --config webpack/common.js",
    "prepublishOnly": "npm run build"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "base64-arraybuffer": "^0.2.0",
    "browser-resolve": "^2.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "localstorage-memory": "^1.0.3",
    "mime": "^2.3.1",
    "nanoid": "2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/base64-arraybuffer": "^0.1.0",
    "@types/jest": "^23.3.14",
    "@types/mime": "^2.0.0",
    "@types/mime-types": "^2.1.0",
    "@types/nanoid": "^1.2.1",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-loader": "^8.0.4",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "fs-readdir-recursive": "^1.1.0",
    "husky": "^1.2.0",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "json-loader": "0.5.7",
    "lint-staged": "^8.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^26.5.4",
    "ts-loader": "^5.2.2",
    "typescript": "^4.2.3",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.12",
    "webpack-glob-entries": "^1.0.1",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "heliu <heliu.fe@bytedance.com>",
  "license": "MIT",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bytedance/byteinspire-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/bytedance/byteinspire-js-sdk/issues"
  },
  "homepage": "https://github.com/bytedance/byteinspire-js-sdk#readme",
  "browser": {
    "./dist/node/index.js": "./dist/inspirecloud.min-0.2.1.js"
  },
  "weapp": {
    "./dist/node/index.js": "./dist/inspirecloud.min-0.2.1.js"
  }
}
